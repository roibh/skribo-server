{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "interface-name": [
            true,
            "never-prefix"
        ],
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-snake-case",
            "allow-leading-underscore",
            "allow-pascal-case"
        ]
    },
    "rulesDirectory": []
}